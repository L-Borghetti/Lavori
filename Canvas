<!DOCTYPE html>
<html>
<body>

<canvas id="myCanvas" width="300" height="300"></canvas>

<script>
var c = document.getElementById("myCanvas");
var ctx = c.getContext("2d");


ctx.beginPath();
ctx.arc(150, 150, 120, 0, 2 * Math.PI);
ctx.stroke();


ctx.beginPath();
ctx.moveTo(150, 40);
ctx.lineTo(180, 120);
ctx.lineTo(260, 120);
ctx.lineTo(200, 170);
ctx.lineTo(220, 250);
ctx.lineTo(150, 200);
ctx.lineTo(80, 250);
ctx.lineTo(100, 170);
ctx.lineTo(40, 120);
ctx.lineTo(120, 120);
ctx.closePath();
ctx.stroke();

</script>

</body>
</html>
